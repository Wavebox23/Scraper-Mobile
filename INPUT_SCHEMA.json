    "models": {
      "title": "ğŸ­ Brand and Models",
      "type": "array",
      "description": "Filter by specific car models. Format: 'BRAND|MODEL' (e.g., 'BMW|320', 'Audi|A4')",
      "editor": "stringList",
      "default": []
    },
    "mileageKmMin": {
      "title": "ğŸ“ Minimum Mileage (km)",
      "type": "integer",
      "description": "Minimum mileage in kilometers",
      "minimum": 0
    },
    "mileageKmMax": {
      "title": "ğŸ“ Maximum Mileage (km)",
      "type": "integer",
      "description": "Maximum mileage in kilometers",
      "minimum": 0
    },
    "powerKwMin": {
      "title": "âš¡ Minimum Power (kW)",
      "type": "integer",
      "description": "Minimum engine power in kilowatts",
      "minimum": 0
    },
    "powerKwMax": {
      "title": "âš¡ Maximum Power (kW)",
      "type": "integer",
      "description": "Maximum engine power in kilowatts",
      "minimum": 0
    },
    "priceMin": {
      "title": "ğŸ’° Minimum Price (EUR)",
      "type": "integer",
      "description": "Minimum price in EUR",
      "minimum": 0
    },
    "priceMax": {
      "title": "ğŸ’° Maximum Price (EUR)",
      "type": "integer",
      "description": "Maximum price in EUR",
      "minimum": 0
    },
    "registrationDateYearMin": {
      "title": "ğŸ“… Minimum Registration Year",
      "type": "integer",
      "description": "Minimum first registration year",
      "minimum": 1900
    },
    "registrationDateYearMax": {
      "title": "ğŸ“… Maximum Registration Year",
      "type": "integer",
      "description": "Maximum first registration year",
      "minimum": 1900
    },
    "sort": {
      "title": "ğŸ”„ Sort Results By",
      "type": "string",
      "description": "How to sort the search results",
