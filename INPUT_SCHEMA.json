{
  "title": "Mobile.de Scraper Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "startUrls": {
      "title": "ğŸ” Search/Details page URL(s)",
      "type": "array",
      "description": "Paste Mobile.de search result URLs or specific listing detail URLs here. You can add multiple URLs.",
      "editor": "requestListSources",
      "prefill": [
        {
          "url": "https://suchen.mobile.de/fahrzeuge/search.html?dam=false&isSearchRequest=true&s=Car&vc=Car"
        }
      ]
    },
    "maxItems": {
      "title": "ğŸ”¢ Maximum number of results",
      "type": "integer",
      "description": "Limit the number of vehicle listings to scrape. Leave empty or set to 0 for unlimited (up to 2000 per search).",
      "default": 20,
      "minimum": 0
    },
    "searchCategory": {
      "title": "ğŸš— Vehicle Category",
      "type": "string",
